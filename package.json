{
  "name": "sms-ui",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "cross-env UMI_ENV=test umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "analyze": "cross-env ANALYZE=1 umi build",
    "reinstall": "rm -r ./node_modules && npm i",
    "lint": "npm run format && npm run lint-eslint",
    "format": "eslint --fix './src/**/*.{js,ts,tsx}'",
    "lint-eslint": "eslint -c .eslintrc.js --ext .ts,.tsx,.js '{src,scripts}/**/*.{ts,tsx,js}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-descriptions": "^1.4.2",
    "@ant-design/pro-form": "^1.13.2",
    "@ant-design/pro-layout": "^6.5.0",
    "@ant-design/pro-table": "^2.20.0",
    "@sentry/react": "^6.2.0",
    "@sentry/tracing": "^6.2.0",
    "@umijs/preset-react": "1.x",
    "antd": "^4.10.0",
    "dayjs": "^1.10.1",
    "echarts": "^5.0.0",
    "lodash-es": "^4.17.20",
    "numeral": "^2.0.6",
    "react-copy-to-clipboard": "^5.0.3",
    "umi": "^3.3.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@sentry/cli": "^1.63.1",
    "@sentry/webpack-plugin": "^1.14.1",
    "@types/echarts": "^4.9.3",
    "@types/lodash-es": "^4.17.3",
    "@types/react": "^16.9.0",
    "@types/react-copy-to-clipboard": "^5.0.0",
    "@types/react-dom": "^16.9.0",
    "@umijs/fabric": "^2.4.10",
    "@umijs/test": "^3.3.3",
    "cross-env": "^7.0.3",
    "eslint": "^7.17.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.6",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
